{
    "routes": [
        {
            "src": "/(.*)",
            "dest": "crop_prediction_using_ML/wsgi.py"
        }
    ],
    "builds": [
        {
            "src": "index.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": {
                "maxLambdaSize": "15mb"
            }
        }
    ]
}